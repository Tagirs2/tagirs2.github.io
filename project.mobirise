tep=\"0.1\" value=\"0.4\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container\">\n        <div class=\"mbr-row\" mbr-class=\"{'mbr-jc-c' : textAlign == 2, 'mbr-jc-e' : textAlign == 3}\">\n            <!-- Title -->\n            <div class=\"title-block mbr-col-sm-12 mbr-col-md-8 \">\n                <h2 class=\"mbr-section-subtitle mbr-fonts-style mbr-regular mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\" mbr-class=\"{'mbr-pb-4' : !showTitle}\">\n                    WE ARE CREATIVE\n                </h2>\n                <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold mbr-white\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title, .btn__block\">\n                    We will give you a positive attitude and look at your a life</h1>\n                <div class=\"btn__block\">\n                <div class=\"align-left mbr-section-btn\" mbr-if=\"showBtn1\" data-toolbar=\"-mbrBtnMove\" mbr-buttons=\"\" mbr-theme-style=\"display-4\"><a class=\"btn btn-lg btn-primary\" href=\"https://mobirise.com\" data-app-placeholder=\"Type Text\">\n            DOWNLOAD FREE E-BOOK</a></div>\n                <div class=\"popup-btn mbr-fonts-style mbr-col-sm-12 mbr-bold mbr-white\" mbr-text=\"\" mbr-theme-style=\"display-4\" on=\"tap:my-lightbox\" role=\"button\" tabindex=\"0\" data-app-placeholder=\"Type Text\" data-app-selector=\".popup-btn\"><span class=\"mbrib-play mbr-iconfont\" mbr-icon=\"\"></span>WATCH VIDEOS</div></div>\n            </div>\n            </div>\n        </div>\n \n    <amp-lightbox id=\"my-lightbox\" layout=\"nodisplay\">\n          <div class=\"lightbox\" on=\"tap:my-lightbox.close\" role=\"button\" tabindex=\"0\">\n            <div class=\"container\">\n    