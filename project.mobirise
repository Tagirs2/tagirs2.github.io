          "_customHTML": "<section class=\"features8\" group=\"Features\" plugins=\"amp-main, amp-bind\">\n    <!--  -->\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"20\" step=\"1\" value=\"3\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"20\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Size Icon\" name=\"sizeIcon\" min=\"1\" max=\"6\" step=\"0.5\" value=\"3\" condition=\"showIcon\">\n        <header>Cards Height</header>\n        <input type=\"range\" inline title=\"Card Height 1\" name=\"cardsHeight1\" min=\"50\" max=\"1000\" step=\"10\" value=\"300\">\n        <input type=\"range\" inline title=\"Card Height 2\" name=\"cardsHeight2\" min=\"50\" max=\"1000\" step=\"10\" value=\"300\">\n        <input type=\"range\" inline title=\"Card Height 3\" name=\"cardsHeight3\" min=\"50\" max=\"1000\" step=\"10\" value=\"300\">\n        <header>Cards</header>\n        <input type=\"range\" inline title=\"Cards\" name=\"amountCards\" min=\"1\" max=\"3\" step=\"1\" value=\"3\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Icon\" name=\"showIcon\">\n        <input type=\"checkbox\" title=\"Icon-2\" name=\"showIcon2\" checked>\n        <input type=\"id\" name=\"CardId\">\n        <input type=\"image\" name=\"card1\" title=\"Card 1 BG Image\" value=\"@PROJECT_PATH@/assets/images/people1.jpg\" condition=\"amountCards > 0\">\n        <input type=\"image\" name=\"card2\" title=\"Card 2 BG Image\" value=\"@PROJECT_PATH@/assets/images/people2.jpg\" condition=\"amountCards > 1\">\n        <input type=\"image\" name=\"card3\" title=\"Card 3 BG Image\" value=\"@PROJECT_PATH@/assets/images/people3.jpg\" condition=\"amountCards > 2\">\n        <input type=\"image\" name=\"card4\" title=\"Card 4 BG Image\" value=\"@PROJECT_PATH@/assets/images/people4.jpg\" condition=\"amountCards > 3\">\n        <header>Color</header>\n        <input type=\"color\" title=\"Image Overlay\" name=\"imgBg\" value=\"#14212B\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"imgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"imgBg\">\n        <input type=\"color\" title=\"Color Card Back\" name=\"colorCard2\" value=\"#b2ccd2\">\n        <input type=\"color\" title=\"Color Icon\" name=\"colorIcon\" value=\"#ffffff\" condition=\"showIcon\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background-1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <!--  -->\n    <!-- Overlay -->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <!-- Container -->\n    <div class=\"container\">\n        <!-- Items -->\n        <div class=\"mbr-row\">\n            <!-- 1 -->\n            <div class=\"scene scene--card mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-4\">\n              <div class=\"card card1\" id=\"{{CardId}}Card1\">\n                  <div class=\"card-face card-face--front mbr-flex align-center front-bg-1\">\n                    <div class=\"wrapper-item mbr-flex align-center\">\n                        <div class=\"card-overlay\"></div>\n                      <div class=\"body-card\">\n                        <div class=\"item__icon\" mbr-i